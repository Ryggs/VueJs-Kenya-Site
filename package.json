{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.21",
    "@nuxt-themes/elements": "^1.0.4",
    "@nuxt-themes/tokens": "^2.0.1",
    "@nuxt-themes/typography": "^1.0.1",
    "@nuxt/content": "npm:@nuxt/content-edge@latest",
    "@nuxt/devtools": "latest",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@tailwindcss/typography": "^0.5.10",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.3",
    "nuxt-icon": "^0.5.0",
    "simple-git-hooks": "^2.9.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{.js,.jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "cobe": "^0.6.3"
  }
}
