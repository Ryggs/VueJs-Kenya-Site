{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@antfu/eslint-config": "1.0.0-beta.21",
    "@nuxt/devtools": "latest",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/tailwindcss": "^6.11.4",
    "@types/mailchimp__mailchimp_marketing": "^3.0.20",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{.js,.jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "nuxt": "^3.11.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  }
}
